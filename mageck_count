#mageck count function in Unix bash#
cd /data/ #set directory to the ones with library files and NGS fastq files
mageck count -l Human_GeCKOv2_Library_combine.csv -n PoolA_PoolB --sample-label WT_A_pd0,KI_A_pd0,WT_A_pd5,KI_A_pd5,WT_A_pd10,KI_A_pd10,WT_A_pd15,KI_A_pd15,WT_B_pd0,KI_B_pd0,WT_B_pd5,KI_B_pd5,WT_B_pd10,KI_B_pd10,WT_B_pd15,WT_B_pd15_2,KI_B_pd15  --fastq PoolA_R1_S1_R1_001_001.fastq PoolB_R5_S4_R1_001_003.fastq PoolA_R1_S1_R1_001_005.fastq PoolB_R5_S4_R1_001_007.fastq PoolA_R1_S1_R1_001_009.fastq PoolB_R5_S4_R1_001_011.fastq PoolA_R1_S1_R1_001_013.fastq PoolB_R5_S4_R1_001_015.fastq PoolB_R2_S3_R1_001_002.fastq PoolA_R6_S2_R1_001_004.fastq PoolB_R2_S3_R1_001_006.fastq PoolA_R6_S2_R1_001_008.fastq PoolB_R2_S3_R1_001_010.fastq PoolA_R6_S2_R1_001_012.fastq PoolB_R2_S3_R1_001_014.fastq PoolB_R2_S3_R1_001_017.fastq PoolA_R6_S2_R1_001_016.fastq --pdf-report #run the mageck count command by setting the library file(-l), setting the output file name (-n), setting sample names (--sample-label) in the same order as the sequencing files (--fastq) 
